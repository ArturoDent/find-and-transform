{
 "name": "find-and-transform",
 "displayName": "Find and Transform",
 "description": "Find text and transform it with case modifiers",
 "version": "0.4.0",
 "preview": true,
 "engines": {
  "vscode": "^1.54.0"
 },
 "publisher": "ArturoDent",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "https://github.com/ArturoDent/find-and-transform"
 },
 "categories": [
  "Other"
 ],
 "activationEvents": [
  "onStartupFinished",
  "onCommand:findInCurrentFile.upcaseAllKeywords",
  "onCommand:findInCurrentFile.upcaseSwap2",
  "onCommand:findInCurrentFile.upcaseSelectedKeywords",
  "onCommand:runInSearchPanel.removeDigits"
 ],
 "main": "./src/extension.js",
 "contributes": {
  "commands": [
   {
    "command": "findInCurrentFile.upcaseAllKeywords",
    "title": "Uppercase all Keywords",
    "category": "Find-Transform"
   },
   {
    "command": "findInCurrentFile.upcaseSwap2",
    "title": "swap iif <==> hello",
    "category": "Find-Transform"
   },
   {
    "command": "findInCurrentFile.upcaseSelectedKeywords",
    "title": "upcaseSelectedKeywords",
    "category": "Find-Transform"
   },
   {
    "command": "runInSearchPanel.removeDigits",
    "title": "Remove digits from Arturo",
    "category": "Find-Transform"
   }
  ],
  "configuration": [
   {
    "properties": {
     "findInCurrentFile": {
      "scope": "resource",
      "description": "Find in the current file."
     },
     "runInSearchPanel": {
      "scope": "resource",
      "description": "Search using the Search Panel."
     }
    }
   }
  ]
 },
 "scripts": {
  "lint": "eslint .",
  "pretest": "npm run lint",
  "test": "node ./test/runTest.js"
 },
 "devDependencies": {
  "@types/vscode": "^1.54.0",
  "@types/glob": "^7.1.1",
  "@types/mocha": "^7.0.2",
  "@types/node": "^13.11.0",
  "eslint": "^6.8.0",
  "glob": "^7.1.6",
  "mocha": "^7.1.2",
  "typescript": "^3.8.3",
  "vscode-test": "^1.3.0"
 },
 "isUserBuiltin": false,
 "isBuiltin": false,
 "isUnderDevelopment": true,
 "id": "ArturoDent.find-and-transform",
 "identifier": {
  "value": "ArturoDent.find-and-transform",
  "_lower": "arturodent.find-and-transform"
 },
 "extensionLocation": {
  "$mid": 1,
  "fsPath": "c:\\Users\\Mark\\find-and-transform",
  "_sep": 1,
  "path": "/c:/Users/Mark/find-and-transform",
  "scheme": "file"
 }
}